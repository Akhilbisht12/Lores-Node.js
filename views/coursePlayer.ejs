<%- include("Partials/header")%>
    <div class="container py-1 px-4">
        <div class="row rounded-lg overflow-hidden shadow">

            <!-- Chat Box-->
            <div class="col-8 px-0">
                <div style="height: 500px;" class="p-1 chat-box bg-white">
                    <iframe width="100%" height="100%" src="https://www.youtube.com/embed/yVwl3E9sKFE" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                </div>
            </div>

            <!-- Users box-->
            <div class="col-4 px-0 border-right">
                <div class="bg-white">

                    <div class="bg-gray px-4 py-2 bg-light">
                        <p class="h5 mb-0 py-1">
                            Course Name
                        </p>
                    </div>
                    <div style="overflow-y: scroll; height: 450px;" class="" data-spy="scroll" data-offset="0" id="teamMessagediv">
                        <% for(var i =0; i<=10;i++){%>
                            <div class="messages-box">
                                <div class="list-group rounded-0 p-2">
                                    <a class="list-group-item list-group-item-action  text-black rounded-0">
                                        <div class="media"><img src="https://cdn.pixabay.com/photo/2017/11/10/05/34/play-2935460_960_720.png" alt="user" width="50" class="rounded-circle">
                                            <div class="media-body ml-4">
                                                <div class="d-flex align-items-center justify-content-between mb-1">
                                                    <h5 class="mb-0">
                                                        video Name
                                                    </h5><small class="small font-weight-bold">25 Dec</small>
                                                </div>
                                                <p class="font-italic mb-0 text-small">video Descriptions that is relatable to the video </p>
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </div>
                            <%}%>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- include("Partials/footer")%>